#foreach($stylist in $stylists)
  <div class="styleCard">
    <p>STYLIST'S NAME</p>
    <br>
    <p class="displayName">$stylist.getStylistName()</p>
    <p>STYLIST'S STYLE</p>
    <p>$stylist.getStyle()</p>
    <a href="/stylist/$stylist.getId()/viewClients"><button>View stylist</button></a>
  </div>
#end

<a href="/"><button>Add New stylist</button></a>